{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "dependencies": [
    "boost-asio",
    "boost-iostreams",
    "boost-locale",
    "boost-lockfree",
    "boost-system",
    "boost-variant",
    "fmt",
    {
      "name": "libiconv",
      "platform": "osx"
    },
    "libmariadb",
    "openssl",
    "pugixml"
  ],
  "features": {
    "http": {
      "description": "Enable HTTP support",
      "dependencies": [
        "boost-beast",
        "boost-json"
      ]
    },
    "lua": {
      "description": "Use Lua instead of LuaJIT",
      "dependencies": [
        "lua"
      ]
    },
    "luajit": {
      "description": "Use LuaJIT instead of Lua",
      "dependencies": [
        "luajit"
      ]
    },
    "unit-tests": {
      "description": "Build unit tests",
      "dependencies": [
        "boost-test"
      ]
    }
  },
  "default-features": [
    "lua",
    "http"
  ],
  "builtin-baseline": "215a2535590f1f63788ac9bd2ed58ad15e6afdff"
}
